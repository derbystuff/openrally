{
  "name": "racemaster",
  "version": "0.0.1",
  "description": "SoapBox Derby Race management",
  "main": "./bin/racemaster",
  "bin": {
    "racemaster": "./bin/racemaster"
  },
  "scripts": {
    "dev": "rm -rf ./web/site/; nodemon ./bin/racemasterdev --hot",
    "test": "lab --transform ./test/setup.js --ignore 'Reflect,core,_babelPolyfill,regeneratorRuntime,__core-js_shared__'",
    "compile-ui": "rm -rf web/site/; NODE_ENV=production webpack --colors --display-error-details --config configs/webpack.client.js",
    "compile": "npm run compile-ui",
    "preuninstall": "rm -rf dist/; rm -rf web/site/",
    "install": "npm run compile",
    "start": "NODE_ENV=production ./bin/racemaster"
  },
